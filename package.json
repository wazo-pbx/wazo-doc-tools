{
  "name": "wazo-doc-api",
  "private": true,
  "description": "Wazo project documentation for developers",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/gatsby build --prefix-paths",
    "develop": "./node_modules/.bin/gatsby develop",
    "format": "./node_modules/.bin/prettier --write src/**/*.{js,jsx} tests/*.js gatsby-node.js",
    "start": "npm run develop",
    "serve": "./node_modules/.bin/gatsby serve",
    "test": "for filename in tests/*.js; do node $filename localhost:8000; done"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "algoliasearch": "^3.33.0",
    "babel-plugin-styled-components": "^1.10.0",
    "core-js": "^3.2.1",
    "gatsby": "^2.4.3",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-source-filesystem": "^2.0.34",
    "gatsby-transformer-yaml": "^2.1.12",
    "js-yaml": "^3.13.1",
    "mobx": "^4.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-in-viewport": "0.0.37",
    "react-instantsearch-dom": "^5.6.0",
    "react-markdown": "^4.0.8",
    "redoc": "2.0.0-rc.14",
    "showdown": "^1.9.0",
    "stickyfill": "^1.1.1-strict",
    "striptags": "^3.1.1",
    "styled-components": "^4.2.0",
    "webpack": "^4.40.1"
  },
  "devDependencies": {
    "gatsby-plugin-sass": "^2.1.8",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wazo-pbx/wazo-doc-api"
  }
}
